{
  "bin": {
    "dsaify": "./out/index.js"
  },
  "dependencies": {
    "@types/react": "^19.1.8",
    "ink": "^6.0.1",
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "lefthook": "^1.12.2"
  },
  "module": "index.tsx",
  "name": "dsaify",
  "peerDependencies": {
    "typescript": "^5"
  },
  "private": true,
  "scripts": {
    "build": "bun build ./src/index.tsx --outdir ./out --minify --sourcemap --target bun",
    "build:watch": "bun run build -- --watch ",
    "link:local": "bun link",
    "link:package": "bun link dsaify",
    "lint:fix": "biome check --write .",
    "prepare": "lefthook install",
    "start:local": "bun run ./out/index.js",
    "start:package": "dsaify",
    "test": "bun tsc && bun test",
    "unlink:local": "bun unlink",
    "update:questions": "bun src/scripts/update-questions.ts"
  },
  "type": "module"
}
